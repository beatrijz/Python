num = int(input ('Digite um valor: '));
if num>=0:
    print ('Positivo');
elif num <= 0:
    print ('Negativo');
    
    
    
nota1 = float(input ('Digite a primeira nota: '));
nota2 = float(input('Digite a segunda nota: '));
media = (nota1 + nota2)/2;
if media >= 7 and media <= 9.9:
    print ('Aprovado')
elif (media == 10):
    print ("Aprovado com Distinção")
else:
    print ('Reprovado');